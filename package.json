        "@turf/turf": ">=5.1.6",
        "@types/node-fetch": "^2.6.2",
        "country-iso-3-to-2": "^1.1.0",
        "deck.gl": "=8.5.2",
        "dotenv": ">=8.2.0",
        "js-aprs-fap": ">=1.0.0",
        "js-aprs-is": ">=1.0.1",
        "kalmanjs": "^1.1.0",
        "lodash": ">=4.17.19",
        "lodash.assignwith": "^4.2.0",
        "lodash.chunk": "^4.2.0",
        "lodash.clone": ">=4.5.0",
        "lodash.clonedeep": ">=4.5.0",
        "lodash.filter": ">=4.6.0",
        "lodash.find": ">=4.6.0",
        "lodash.flatmap": "^4.5.0",
        "lodash.foreach": ">=4.5.0",
        "lodash.foreachright": "^4.4.0",
        "lodash.groupby": ">=4.6.0",
        "lodash.isequal": "^4.5.0",
